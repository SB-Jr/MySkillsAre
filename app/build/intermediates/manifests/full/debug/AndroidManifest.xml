<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.futurefirst.sbjr.myskillsare"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.futurefirst.sbjr.myskillsare.Login"
            android:windowSoftInputMode="adjustPan" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.futurefirst.sbjr.myskillsare.Register" />
        <activity
            android:name="com.futurefirst.sbjr.myskillsare.CreateProfile"
            android:parentActivityName="com.futurefirst.sbjr.myskillsare.Register" />
        <activity
            android:name="com.futurefirst.sbjr.myskillsare.EditSkill"
            android:parentActivityName="com.futurefirst.sbjr.myskillsare.CreateProfile" />
        <activity android:name="com.futurefirst.sbjr.myskillsare.SkillView" />
        <activity android:name="com.futurefirst.sbjr.myskillsare.EditProfile" />
        <activity android:name="com.futurefirst.sbjr.myskillsare.Profile" />
        <activity android:name="com.futurefirst.sbjr.myskillsare.Search" />
    </application>

</manifest>